def classificador(a, b, c):
     e_triangulo = (a + b) > c
    
    if not e_triangulo: 
        print("gondim sendo gondim")
        return None
        
    print ("triangulo")
    
    if (a != b) and (a != c) and (b != c):
        print("escaleno")
        
    if (a == b) or (b == c):
        print("isosceles")
        
    if (a == b) and (b == c):
        print("equilatero")
        
    if (a >= b) and (b >= c):
        if a**2 == ((b**2) + (c**2))