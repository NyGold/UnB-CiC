palindromo = input()

qtd_letras_diferentes = 0

metade_indice = (len(palindromo) - 1) // 2

if len(palindromo) % 2 == 0:
        
    contador = len(palindromo) - 1
    
    for carac in palindromo:
        if contador > len
            if carac != palindromo[contador]:
                qtd_letras_diferentes += 1
            contador -= 1


elif len(palindromo) % 2 != 0:

    qtd_letras_diferentes += 1

    for i in palindromo[:metade_indice]:
        
        contador = len(palindromo) - 1
        
        while contador > metade_indice:
            if i != palindromo[contador]:
                qtd_letras_diferentes += 1
                
            contador -= 1
            break

 
if qtd_letras_diferentes == 1:
    print("ON")
else:
    print("OFF")
    